<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.SampleRestaurantApp.SampleRestaurantApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SampleRestaurantApp.SampleRestaurantApp.MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SampleRestaurantApp.SampleRestaurantApp.SelectCusine" />
        <activity android:name="com.example.SampleRestaurantApp.SampleRestaurantApp.Restaurant_list" />
        <activity android:name="com.example.SampleRestaurantApp.SampleRestaurantApp.Menu_list" />
        <activity android:name="com.example.SampleRestaurantApp.SampleRestaurantApp.Payment"></activity>
    </application>

</manifest>